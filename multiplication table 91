import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();

        for(int i = 1; i <= 10; i++) {
            System.out.print(i + "*" + N + "=" + (i * N));
            if(i < 10) {
                System.out.println(" ");
            }
        }
    }
}
