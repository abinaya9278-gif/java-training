import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) 
    {
    Scanner s=new Scanner(System.in);
    String str=s.nextLine();
      String add=s.nextLine();
      int a=s.nextInt();
        
            StringBuffer obj=new StringBuffer(str);
        obj.insert(a,add);
        System.out.print(obj);
    }
}
