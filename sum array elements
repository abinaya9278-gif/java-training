import java.util.Scanner;

 public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);

        int N = s.nextInt();  
        long sum = 0;          

        for (int i = 0; i < N; i++) 
        {
            int num = s.nextInt();
            sum +=num;
        }

        System.out.print(sum);
    }
}

