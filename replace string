import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) 
    {
    Scanner s=new Scanner(System.in);
    String str=s.nextLine();
        String add=s.nextLine();
        int a=s.nextInt();
        int b=s.nextInt();
      StringBuffer obj=new StringBuffer(str);
        obj.replace(a,b,add);
        System.out.print(obj);
    }
}
